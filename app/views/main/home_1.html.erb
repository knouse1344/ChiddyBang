				$(pinger).attr("style", "width: 0px; height: 0px; opacity: 1;")
			}, 500)
			musicduration = track.durationEstimate;
			musicduration = musicduration.toFixed(2);
			musicposition = track.position;
			musicposition = musicposition.toFixed(2);
			percent = (musicposition/musicduration)*100;
			percent = percent.toFixed(2);

			if (playmemory == 0) {
		  			track.play();
		  			progressbar(percent, true);
		  			playmemory = 1;
		  			$('.playing-gif').css("opacity", "1");
			} else { 
		  			track.pause();
		  			playmemory = 0;
		  			progressbar(percent, false);
		  			clearInterval(spinInterval);
		  			$('.playing-gif').css("opacity", "0");
			}
			

			if ($(this).hasClass('playing')) {
				$(this).removeClass();
				$(this).addClass('fa fa-play');
			} else {
				$(this).removeClass();
				$(this).addClass('fa fa-pause playing');     		
			};
		});

		var spinner = $('.spinner');
		var degrees = 0;
		var spinInterval;

		function rotate(percy, conty) {
			
			if (conty == false) {
				$(spinner).attr("style", "-webkit-transform: rotate("+ percy +"deg); -moz-transform: rotate("+ percy +"deg); -ms-transform: rotate("+ percy +"deg); transform: rotate("+ percy +"deg);");
				clearInterval(spinInterval);
			} else {
				spinInterval = setInterval(function () {
					musicduration = track.durationEstimate;
					musicduration = musicduration.toFixed(2);
					musicposition = track.position;
					musicposition = musicposition.toFixed(2);
					percent = (musicposition/musicduration)*100;
					percent = percent.toFixed(2);
					percent = parseFloat(percent);
					//percent += 0.21;

					percent = percent*3.6;
					console.log(percent);
					console.log(conty);

					$(spinner).removeAttr("style");
					$(spinner).attr("style", "-webkit-transform: rotate("+ percent +"deg); -moz-transform: rotate("+ percent +"deg); -ms-transform: rotate("+ percent +"deg); transform: rotate("+ percent +"deg);");
				}, 300);
			}
		}
		

		function progressbar(percent, continuespin) {

			degrees = parseFloat(degrees);
			percent = parseFloat(percent);
			percent = percent*3.6;
			rotate(percent, continuespin);
		}
	});

