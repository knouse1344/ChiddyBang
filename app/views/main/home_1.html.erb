<script>
	$(document).ready(function() {
		var navi = ('.nav-bottom');
		var slidein = ('.slide-in');
		var news = ("#news");
		var navnews = (".nav-news");
		var nstory1 = (".news-story-1");
		var nstory2 = (".news-story-2");
		var nstory3 = (".news-story-3");
		var music = ("#music");
		var navmusic = (".nav-music");
		var shows = ("#shows");
		var navshows = ('.nav-shows');
		var about = ("#about");
		var navabout = (".nav-about");
		var navli = ("#index .nav-container .slide-in ul li");
		var crown = (".crown");
		var thumb = (".instagram-pics");

		var fbicon = (".fbicon");
		var twicon = (".twicon");
		var inicon = (".inicon");
		var piicon = (".piicon");

		var playbutton = (".fa-play");
		var playmemory = 0;

		//secret 57964264577527dced5b10abb8aa7fd0
		// Soundcloud API schtuffs
		SC.initialize({
		  	client_id: 'c73eaa253b16c4cc3323dad047b74b27'
		});

		var musicwrap = $('.music-player');
		var musicduration = 0;
		var musicposition = 0;
		var percent = 0;
		var progresswidth = $(".pbar");

		var rotation = 360;
		var vinyl = $('.vinyl');
		var stylus = $('.vinyl-stylus');

		var navstate = $('.nav-state ul');

		$(function() {

			$.ajax({
			    type: "GET",
			    dataType: "jsonp",
			    cache: false,
			    user: "realchiddy",
			    success: function(data) {
			        for (var i = 0; i < 1; i++) {
			          console.log(data);
			            $("#pics").append("<img src='" + data.data.images.thumbnail.url+ "'></img>");
			        }
			    }
			});
		});

		$('a[href*=#]:not([href=#])').click(function() {
		    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') 
		        || location.hostname == this.hostname) {

		        var target = $(this.hash);
		        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
		           if (target.length) {
		             $('html,body').animate({
		                 scrollTop: target.offset().top + 5
		            }, 750);
		            return false;
		        }
		    }
		});

		$(navi).waypoint(function(direction) {
			if (direction == 'down') {
		  		$(this).addClass('nav-fixed');
		  		$(crown).attr("style", "top: 0;");
		  	} else {
		  		$(this).removeClass('nav-fixed');
		  		$(crown).attr("style", "top: -50px;");
		  	}
		});

		$(news).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navnews).addClass('active');
				navstate.addClass('news');
				$(nstory1).addClass('active');
			} else {
				$(navnews).removeClass('active');
				navstate.removeClass();
				$(nstory1).removeClass('active');
			}
		})
		$(nstory1).waypoint(function(direction) {
			if (direction == 'down') {
				$(nstory2).addClass('active');
				$(nstory1).removeClass('active');
			} else {
				$(nstory1).addClass('active');
				$(nstory2).removeClass('active');
			}
		})
		$(nstory2).waypoint(function(direction) {
			if (direction == 'down') {
				$(nstory3).addClass('active');
				$(nstory2).removeClass('active');
			} else {
				$(nstory2).addClass('active');
				$(nstory3).removeClass('active');
			}
		})
		$(shows).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navshows).addClass('active');
				navstate.removeClass();
				navstate.addClass('shows');
			} else {
				$(navshows).removeClass('active');
				$(navnews).addClass('active');
				navstate.removeClass();
				navstate.addClass('news');
			}
		})
		$(music).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navmusic).addClass('active');
				navstate.removeClass();
				navstate.addClass('music');
			} else {
				$(navmusic).removeClass('active');
				$(navshows).addClass('active');
				navstate.removeClass();
				navstate.addClass('shows');
			}
		})
