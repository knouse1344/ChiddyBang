<script>
	$(document).ready(function() {
		var navi = ('.nav-bottom');
		var slidein = ('.slide-in');
		var news = ("#news");
		var navnews = (".nav-news");
		var music = ("#music");
		var navmusic = (".nav-music");
		var shows = ("#shows");
		var navshows = ('.nav-shows');
		var about = ("#about");
		var navabout = (".nav-about");
		var navli = ("#index .nav-container .slide-in ul li");
		var thumb = (".instagram-pics");

		var fbicon = (".fbicon");
		var twicon = (".twicon");
		var inicon = (".inicon");
		var piicon = (".piicon");

		var playbutton = (".play-button");

		var iframeElement   = document.getElementById('soundcloud-container');
		var iframeElementID = iframeElement.id;
		var player         = SC.Widget(iframeElement);
		var widget2         = SC.Widget(iframeElementID);

		var musicwrap = $('.music-player');
		var musicduration = 0;
		var musicposition = 0;
		var percent = 0;
		var progresswidth = $(".pbar");

		var rotation = 360;
		var vinyl = $('.vinyl');
		var stylus = $('.vinyl-stylus')

		$(function() {

			$.ajax({
			    type: "GET",
			    dataType: "jsonp",
			    cache: false,
			    user: "realchiddy",
			    success: function(data) {
			        for (var i = 0; i < 1; i++) {
			          console.log(data);
			            $("#pics").append("<img src='" + data.data.images.thumbnail.url+ "'></img>");
			        }
			    }
			});
		});

		$('a[href*=#]:not([href=#])').click(function() {
		    if (location.pathname.replace(/^\//,'') == this.pathname.replace(/^\//,'') 
		        || location.hostname == this.hostname) {

		        var target = $(this.hash);
		        target = target.length ? target : $('[name=' + this.hash.slice(1) +']');
		           if (target.length) {
		             $('html,body').animate({
		                 scrollTop: target.offset().top
		            }, 750);
		            return false;
		        }
		    }
		});

		$(navi).waypoint(function(direction) {
			if (direction == 'down') {
		  		$(this).addClass('nav-fixed');
		  		$(slidein).css('left', "0");
		  	} else {
		  		$(this).removeClass('nav-fixed');
		  		$(slidein).css('left', "-300px");
		  	}
		});

		$(news).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navnews).addClass('active');
			} else {
				$(navnews).removeClass('active');
			}
		})
		$(music).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navmusic).addClass('active');
			} else {
				$(navmusic).removeClass('active');
				$(navnews).addClass('active');
			}
		})
		$(shows).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navshows).addClass('active');
			} else {
				$(navshows).removeClass('active');
				$(navmusic).addClass('active');
			}
		})
		$(about).waypoint(function(direction) {
			if (direction == 'down') {
				$(navli).removeClass('active');
				$(navabout).addClass('active');
			} else {
				$(navabout).removeClass('active');
				$(navshows).addClass('active');
			}
		})

		$(".news").mCustomScrollbar({
			theme:"light-3",
			scrollInertia: 150
		});

		$(thumb).hover(function() {
			$(thumb).addClass("hover");
		}, function() {
			$(thumb).removeClass("hover");
		});

		$(fbicon).hover(function () {
			$(this).attr("style", "background: url('/assets/face-smoke.gif?rnd="+Math.random()+"'); background-position: center; background-size: 100%;");
			setTimeout(function() {
				$(".fbicon-img").attr("style", "opacity: 0;");
			},10);
		}, function () {
			$(".fbicon-img").attr("style", "opacity: 1;");
			$(this).removeAttr('style');
		});
		$(twicon).hover(function () {
			$(this).attr("style", "background: url('/assets/twit-smoke.gif?rnd="+Math.random()+"'); background-position: center; background-size: 100%;");
			setTimeout(function() {
				$(".twicon-img").attr("style", "opacity: 0;");
			},10);
		}, function () {
			$(".twicon-img").attr("style", "opacity: 1;");
			$(this).removeAttr('style');
		});
		$(inicon).hover(function () {
			$(this).attr("style", "background: url('/assets/inst-smoke.gif?rnd="+Math.random()+"'); background-position: center; background-size: 100%;");
			setTimeout(function() {
				$(".inicon-img").attr("style", "opacity: 0;");
			},10);
		}, function () {
			$(".inicon-img").attr("style", "opacity: 1;");
			$(this).removeAttr('style');
		});
		$(piicon).hover(function () {
			$(this).attr("style", "background: url('/assets/pint-smoke.gif?rnd="+Math.random()+"'); background-position: center; background-size: 100%;");
			setTimeout(function() {
				$(".piicon-img").attr("style", "opacity: 0;");
			},10);
		}, function () {
			$(".piicon-img").attr("style", "opacity: 1;");
			$(this).removeAttr('style');
		});

		$(playbutton).on("click", function () {

			player.getDuration(function(duration){
				musicduration = duration;
				musicduration = musicduration.toFixed(2);
			});

			if ($(this).hasClass('playing')) {
				$(this).removeClass('playing');
				player.pause();
			} else {
				$(this).addClass('playing');
			  	player.play();
			  	player.getPosition(function(position){
					musicposition = position;
					musicposition = musicposition.toFixed(2);
					percent = (musicposition/musicduration)*100;
					percent = percent.toFixed(2);
					progressbar(percent);
				});        		
			};
		});

		function progressbar(percent) {
			setInterval(function() {
				percent = parseFloat(percent);
				percent += 0.21;

				progresswidth.attr("style", "width:"+percent+"%;");
			}, 500);
		}


		function rotateshows() {
			// var stylusrotate = 90;

