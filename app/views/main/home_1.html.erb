		$(playbutton).on("click", function () {

			$(pinger).attr("style", "width: 300px; height: 300px; opacity: 0;");
			setTimeout(function() {
				$(pinger).attr("style", "width: 0px; height: 0px; opacity: 0;")
			}, 300)
