		$(playbutton).on("click", function () {

			musicduration = track.durationEstimate;
			musicduration = musicduration.toFixed(2);
			musicposition = track.position;
			musicposition = musicposition.toFixed(2);
			percent = (musicposition/musicduration)*100;
			percent = percent.toFixed(2);
			progressbar(percent);

			if (playmemory == 0) {
		  			track.play();
		  			playmemory = 1;
			} else { 
		  			track.pause();
		  			playmemory = 0;
		  			$('.playing-gif').remove();
			}
			

			if ($(this).hasClass('playing')) {
				$(this).removeClass();
				$(this).addClass('fa fa-play');
			} else {
				$(this).removeClass();
				$(this).addClass('fa fa-pause playing');     		
			};
		});

